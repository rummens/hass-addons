{
  "name": "MongoDB",
  "description": "MongoDB NoSQL database server.",
  "version": "8.2.3-6",
  "slug": "mongodb",
  "arch": ["amd64"],
  "map": [
    {
      "config": {
        "source": "/share/mongodb",
        "target": "/data/db",
        "type": "rw"
      }
    },
    "share:rw"
  ],
  "ports": {
    "27017/tcp": 27017
  },
  "options": {
    "root_username": "root",
    "root_password": null,
    "init_scripts": false
  },
  "schema": {
    "root_username": "str",
    "root_password": "password",
    "init_scripts": "bool?"
  },
  "startup": "service",
  "boot": "auto"
}
